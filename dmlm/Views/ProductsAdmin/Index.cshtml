@model IEnumerable<dmlm.Product>

@{
    Layout = null;
    Layout = ViewBag.Layout;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.createDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.urlPrimaryImage)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.urlProduct)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.upc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.partNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.stockingCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.serviceProviderId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.updateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.endDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProductCategory.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UOM.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Manufacturer.name)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.createDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.urlPrimaryImage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.urlProduct)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.upc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.partNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.stockingCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.serviceProviderId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.updateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.endDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isActive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProductCategory.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UOM.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Manufacturer.name)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
